{
    "manifest_version": 2,
    "name": "onlyAudioYT",
    "version": "1.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
      "48": "icons/border-48.png"
    },

    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["onlyAudio.js"]
      }
    ]
}
